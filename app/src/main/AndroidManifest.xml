<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="elearning.vieted.com.vietedelearning" >

    <!-- Permission - Internet Connect -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- Network State Permissions -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_vieted"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="elearning.vieted.com.activity.VietEDActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="elearning.vieted.com.activity.Course_Syllabus_Activity"
            android:label="@string/title_activity_course__syllabus_" >
        </activity>
    </application>

</manifest>
